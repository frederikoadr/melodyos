{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "venv/**,.venv/**,env/**,node_modules/**,tests/**,__pycache__/**,*.pyc,.git/**,docs/**"
    }
  }
}
